autodetach on
defautonuke on
deflogin off
defmode 0660
defmonitor off
defscrollback 256
defsilence off
defwrap on
escape ^Rr
nethack off
startup_message off
shelltitle ""
hardstatus alwaysignore
termcapinfo xterm*|rxvt*|kterm*|Eterm* hs:ts:fs:ds

defutf8 on
defencoding UTF-8

bind ^\
bind \\
bind \^
bind P
bind R
bind I
bind O
bind z
bind ^Z

bind + select

bind -    select 10
bind !    select 11
bind @    select 12
bind \#   select 13
bind \$   select 14
bind %    select 15
bind \^   select 16
bind &    select 17
bind *    select 18
bind (    select 19
bind )    select 20
bind `    select 21
bind ~    select 22

screen -t ""       1
screen -t ""       2
screen -t ""       3

chdir
