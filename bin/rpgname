if(grep /\-/, @ARGV)
{
  print "Usage: $0 [kind] [kind] [...]\n";
  print wrap("  ", "  ", join(' ', (sort keys %{$nam}))) . "\n";
  exit 0;
}

push @ARGV, sort keys %{$nam} if(!@ARGV);

for my $key (sort keys %{$nam})
{
  if(grep /^$key$/, @ARGV)
  {
    print ucfirst($nam->{$key}->{'s'}->[int(rand(@{$nam->{$key}->{'s'}}))]) if(defined $nam->{$key}->{'s'});
    print $nam->{$key}->{'m'}->[int(rand(@{$nam->{$key}->{'m'}}))] if(defined $nam->{$key}->{'m'});
    print $nam->{$key}->{'e'}->[int(rand(@{$nam->{$key}->{'e'}}))] if(defined $nam->{$key}->{'e'}); print "\n";
  }
}

